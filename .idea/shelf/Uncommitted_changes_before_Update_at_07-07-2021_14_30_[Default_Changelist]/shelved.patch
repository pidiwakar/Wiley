+package Wiley2;
+
+import java.util.Comparator;
+import java.util.List;
+
+public class Employee extends User {
+    Address address;
+    Project project;
+    double salary;
+
+
+    public Employee() {}
+    
+    public Employee(String id, String name) {
+        super(id, name);
+    }
+
+    public Employee(String id, String name, Address address, Project project, double salary) {
+        super(id, name);
+        this.address = address;
+        this.project = project;
+        this.salary = salary;
+    }
+
+    public Employee(User user) {
+    }
+
+    @Override
+    public String toString() {
+        return "Employee{" +
+                "address=" + address +
+                ", project=" + project +
+                ", salary=" + salary +
+                ", id='" + id + '\'' +
+                ", name='" + name + '\'' +
+                '}';
+    }
+
+    public  static void display(List<Employee> l) {
+        for(Employee e:l) {
+            System.out.println(e.id+" "+e.name+" "+e.address+" "+e.project+" "+e.salary);
+        }
+    }
+
+}
+
+class EmpSortBySal implements Comparator<Employee> {
+
+    @Override
+    public int compare(Employee e1, Employee e2) {
+         double res = e1.salary-e2.salary;
+         if(res<0)
+             return -1;
+         if (res>0)
+             return 1;
+         return 0;
+    }
+}
