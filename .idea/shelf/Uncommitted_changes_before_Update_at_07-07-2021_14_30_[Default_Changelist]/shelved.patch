+package Wiley2;
+
+import java.util.*;
+import java.util.stream.Collectors;
+
+public class Main {
+    public static void main(String[] args) {
+
+        Project p1 = new Project(1,"Google",10000);
+        Project p2 = new Project(2,"oracle",15000);
+        Project p3 = new Project(3,"Adobe",20000);
+
+        Address a1 = new Address("Bang","562157");
+        Address a2 = new Address("Bang","500143");
+        Address a3 = new Address("Del","100234");
+
+
+        List<User> u = new ArrayList<>();
+
+        u.add(new User("a1","Rahul"));
+        u.add(new Employee("e1","Piyush",a1,p1,12000));
+       u.add(new Employee("e2","Mohit",a2,p1,10000));
+        u.add(new Employee("e3","Dodo",a3,p1,5000));
+        u.add(new User("a2","Gattu"));
+        u.add(new Employee("e4","Kanishk",a3,p2,13000));
+        u.add(new Employee("e5","Fatty",a3,p2,10000));
+        u.add(new Employee("e5","Simran",a1,p2,4000));
+
+        List<Employee> l = new ArrayList<>();
+
+     //l=u.stream().filter(e->e instanceof Employee).map(u(Employee)).collect(Collectors.toList());
+
+        for (Object u1:u) {
+            if(u1 instanceof Employee)
+                l.add((Employee) u1);
+        }
+        Map<Project,List<Employee>> m= new HashMap<>();
+
+        m.put(p1,l.stream().filter(e -> e.project==p1).collect(Collectors.toList()));
+        m.put(p2,l.stream().filter(e -> e.project==p2).collect(Collectors.toList()));
+        m.put(p3,l.stream().filter(e -> e.project==p3).collect(Collectors.toList()));
+        for(Project p:m.keySet()) {
+            double b = p.budget;
+            double sum=0;
+            Collections.sort(m.get(p),(e1,e2)-> (int) (e1.salary-e2.salary));
+            for(Employee e:m.get(p)) {
+                sum+=e.salary;
+            }
+        }
+
+    }
+}
